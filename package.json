{
  "name": "neon-sdk",
  "version": "0.0.0",
  "description": "TypeScript SDK for managing your Neon Serverless PostgreSQL projects",
  "keywords": [
    "neon",
    "sdk",
    "postgresql",
    "serverless"
  ],
  "homepage": "https://github.com/paambaati/neon-js-sdk",
  "bugs": {
    "url": "https://github.com/paambaati/neon-js-sdk/issues"
  },
  "license": "MIT",
  "author": {
    "name": "GP",
    "email": "me@httgp.com",
    "url": "https://httgp.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/paambaati/neon-js-sdk.git"
  },
  "config": {
    "neonOpenApiSpec": "https://dqjnwjfwjj8yz.cloudfront.net/api_spec/release/v2.json"
  },
  "scripts": {
    "generate": "npx openapi-typescript-codegen --useUnionTypes --name NeonClient --input $npm_package_config_neonOpenApiSpec --output ./generated",
    "build": "tsup ./generated/NeonClient.ts --dts --format cjs,esm --target node18",
    "prepublishOnly": "npm run build"
  },
  "engines": {
    "node": ">=18",
    "deno": ">=1.28"
  },
  "engineStrict": true,
  "files": [
    "dist/"
  ],
  "main": "./dist/NeonClient.js",
  "module": "./dist/NeonClient.mjs",
  "types": "./dist/NeonClient.d.ts",
  "exports": {
    ".": {
      "types": "./dist/NeonClient.d.ts",
      "require": "./dist/NeonClient.js",
      "import": "./dist/NeonClient.mjs"
    }
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "tag": "latest"
  },
  "devDependencies": {
    "@semantic-release/changelog": "6.0.2",
    "@semantic-release/commit-analyzer": "9.0.2",
    "@semantic-release/git": "10.0.1",
    "@semantic-release/github": "8.0.7",
    "@semantic-release/npm": "9.0.2",
    "@semantic-release/release-notes-generator": "10.0.3",
    "openapi-typescript-codegen": "0.23.0",
    "tsup": "6.6.0",
    "typescript": "4.9.5"
  }
}
